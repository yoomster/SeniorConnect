@page 
@model SeniorConnect.WebApp.Pages.ProfilePages.AllUsersModel
@using System.Linq

@{
    ViewData["Title"] = "Alle gebruikers";
}

 
@if (Model.Users != null && Model.Users.Count() > 0)
 {
     <table class="table">
         <thead>
             <tr>
                 <th>ID</th>
                 <th>First Name</th>
                 <th>Last Name</th>
             </tr>
         </thead>
         <tbody>
            @foreach (var user in Model.Users)
             {

                 <tr>
                     <td>@user.Id</td>
                     <td>@user.FirstName</td>
                     <td>@user.LastName</td>
                 </tr>
             }
         </tbody>
     </table>
 } 
 else
 {
     <p>No users found.</p>
 }


